//shankul khare
//Find max in a tree a tree
//20:20
struct node
{
    int data;
    node*left;
    node*right;
};
node*NewNode(int data)
{
    //node*temp;
    node *temp=new node();
    temp->data=data;
    temp->left=NULL;
    temp->right=NULL;
    return temp;
}
int max_in_a_tree(node*root)
{

    if(root==NULL)
        return -1;
    int ml=0,mr=0,rr=root->data;
    ml=max_in_a_tree(root->left);
    mr=max_in_a_tree(root->right);
    if(ml>mr)
        mr=ml;
    if(mr<rr)
        mr=rr;

    return mr;

}

int main()
{

    node *A=NewNode(1);
    A->left=NewNode(2);
    A->left->left=NewNode(4);
    A->left->right=NewNode(5);
    A->right=NewNode(3);
    cout<<max_in_a_tree(A);
    return 0;
}
